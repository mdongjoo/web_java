package day01;

import java.util.Scanner;

public class Ex14 {
	public static void main(String[] args) {
//		14. 사용자로부터 세 개의 정수형태의 문자열을 입력받아 문자열,
//		정수, 실수를 출력하기 (.next)만 사용하기
		//로직
		//Scanner import
		//String 변수 3개 선언후 입력받기
		Scanner scan = new Scanner(System.in);
		System.out.print("세 개의 정수를 입력하세요 :");
		String sStr = scan.next(); 
		int iStr = Integer.parseInt(scan.next());
		double dStr = Double.parseDouble(scan.next());;
		
		System.out.println(sStr);
		System.out.println(iStr);
		System.out.println(dStr);
		
		scan.close();
		
	}
}
